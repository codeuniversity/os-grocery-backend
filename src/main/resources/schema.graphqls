type ListItem {
    id: ID!
    name: String!
}

type Supermarket {
    id: ID!
    name: String!
}

type Query {
    getAllListItems: [ListItem]
    getOneListItem(id: ID!): ListItem
}

type Mutation {
    createListItem(name: String!): ListItem
    updateListItem(id: ID!, name: String!): ListItem
    deleteListItem(id: ID!): String

    createSupermarket(name: String!): Supermarket
}

schema {
    query: Query
    mutation: Mutation
}